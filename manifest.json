{
    "name": "Whats My IP",
    "version": "1.1",
    "author": "Vadagon",
    "manifest_version": 2,
    "description": "It lets you check your IP address with just one click. Reveal and Now Show your IP.",
    "browser_action": {
        "default_icon": {
            "128": "images/logo.png"
        },
        "default_popup": "popup.html",
        "default_title": "Show your IP"
    },
    "icons": {
        "128": "images/logo.png"
    },
    "web_accessible_resources": [
        "images/*",
        "fonts/*",
        "scripts/*"
    ]
}